<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\Entity\Apadrinamientos" table="apadrinamientos">
    <indexes>
      <index name="id_usuario" columns="id_usuario"/>
      <index name="id_proyecto" columns="id_proyecto"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="fechaApadrinamiento" type="datetime" column="fecha_apadrinamiento" nullable="false">
      <options>
        <option name="default">CURRENT_TIMESTAMP</option>
      </options>
    </field>
    <many-to-one field="idProyecto" target-entity="Proyectos" fetch="LAZY">
      <join-columns>
        <join-column name="id_proyecto" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idUsuario" target-entity="Usuarios" fetch="LAZY">
      <join-columns>
        <join-column name="id_usuario" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
